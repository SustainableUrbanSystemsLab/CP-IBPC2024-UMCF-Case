FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue LAD 0
);

regions
(
    // Set cell values
    // (does zerogradient on boundaries)
    zoneToCell
    {
        name trees;
        fieldValues
        (
	        volScalarFieldValue LAD 1.0
        );
    }   
);

